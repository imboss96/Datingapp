{
  "name": "dating-app",
  "version": "1.0.0",
  "description": "Dating App with Frontend and Backend",
  "scripts": {
    "start": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd Datingapp && npm run dev",
    "dev:backend": "cd Datingapp/backend && npm run dev",
    "install:all": "cd Datingapp && npm install && cd backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}